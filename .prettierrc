{
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "printWidth": 100,
  "bracketSpacing": true,
  "arrowParens": "always",
  "overrides": [
    {
      "files": "*.jsonc",
      "options": {
        "parser": "jsonc",
        "trailingComma": "all"
      }
    }
  ]
}
